<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">
  <Id>86521247-6328-47c8-8bcf-7abc2749e2a2</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Tarun Excel Tools</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Excel Graph API Tester"/>
  <Description DefaultValue="Fetch Microsoft Graph profile using Excel Add-in"/>
  <IconUrl DefaultValue="https://localhost:3000/assets/icon-32.png"/>
  <SupportUrl DefaultValue="https://tb879.github.io/Plug"/>
<Requirements>
  <Sets DefaultMinVersion="1.3">
    <Set Name="IdentityAPI"/>
  </Sets>
</Requirements>
  <Hosts>
    <Host Name="Workbook" />
  </Hosts>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://tb879.github.io/Plug/src/taskpane/taskpane.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <WebApplicationInfo>
    <Id>86521247-6328-47c8-8bcf-7abc2749e2a2</Id>
    <Resource>https://graph.microsoft.com</Resource>
    <Scopes>https://graph.microsoft.com/User.Read</Scopes>
  </WebApplicationInfo>
</OfficeApp>
